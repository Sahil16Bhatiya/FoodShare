  padding-top: 80px;
  padding-bottom: 80px;
  background: white;
  display: flex;
  align-items: center;
  flex-direction: column;
}

#form {
  display: flex;
  flex-direction: row;
}

#email {
  width: 250px;
  height: 40px;
  border-radius: 5px 0 0 5px;
  outline: none;
  border: 1px solid red;
  text-decoration: none;
}

#submit {
  width: 100px;
  height: 40px;
  border: none;
  background: red;
  color: white;
  font-family: "Josefin Sans", sans-serif;
  border-radius: 0 5px 5px 0;
  font-weight: 500;
  font-size: 16px;
  text-decoration: none;
}

input[type="email"] {
  padding-left: 10px;
}

.hero-newsletter {
  margin-bottom: 20px;
}



@media only screen and (max-width: 984px) {
