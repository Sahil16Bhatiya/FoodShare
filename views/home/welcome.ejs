  vertical-align: initial;
}

.month {
  font-size: 16px;
  font-weight: normal;
}

.pricing-button {
  background-color: white;
  border: 1px solid red;
  border-radius: 2em;
  color: red;
  cursor: pointer;
  padding: .75em 1.5em;
  font-size: 1em;
